@model IEnumerable<DocotorDemo.Models.Tbl_Categories>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetCategoriesByName1</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body style="background-color:red;">

    <div class="table">
        <div class="row">
            <div class="thead-dark text-center alert-danger">
                هذا الموقع للحجزوات لمجموعة من المستشفيات والعيادات
            </div>

        </div>

        @foreach (var item in Model)
        {
            <div class="row">



                <div class="card" style="width: 18rem;">
                    <img src="~/Content/Images/H1.jpg" class="card-img-top" height="50" width="50" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">   @Html.DisplayFor(modelItem => item.CategoryName)</h5>
                        <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                        <a href="#" class="btn btn-primary">Go somewhere</a>
                    </div>
                </div>
            </div>
        }

    </div>
</body>
</html>
